{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
  "installCommand": "pip install poetry && poetry install --no-dev"
}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "build": {
      "env": {
        "PYTHON_VERSION": "3.10"
      }
    }
  }
  